# VistaTrek Backend Environment Variables

# Server
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Database
DATABASE_URL=sqlite:///./vistatrek.db

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# External APIs (optional)
LLM_API_KEY=your_api_key_here
